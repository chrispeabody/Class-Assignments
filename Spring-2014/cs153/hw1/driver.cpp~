#include <iostream>
#include "gmatrix.h"
using namespace std;

int main()
{
  GMatrix kris(3, 4, 2, 3);
  GMatrix chris(2, 2, 4, 7);

  cout<<"A 3 by 4:"<<endl;
  cout<<kris<<endl;
  cout<<"A 2 by 2:"<<endl<<chris<<endl;

  kris = chris;
  cout<<"Setting first to equal second. Displaying first."<<endl<<kris<<endl;

  return 0;
}